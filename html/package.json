{
  "name": "Legrand",
  "description": "Package de gestion du site Legrand",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "npm run wpd | grunt fw",
    "wpd": "webpack --watch --config webpack.config.dev.js",
    "wp": "grunt uglify && webpack --config webpack.config.prod.js",
    "build": "npm run wp && grunt build",
    "css": "grunt s",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "js-toggler": "^1.1.0",
    "nu6tils": "git+https://github.com/romuleald/nu6tils.git#1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "cssnano": "^3.7.0",
    "grunt": "^1.0.1",
    "grunt-combine-media-queries": "^1.0.20",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^1.2.0",
    "webpack": "^1.13.1"
  }
}
