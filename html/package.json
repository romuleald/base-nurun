{
  "name": "bootrun",
  "description": "Package boostrap from Nurun-Paris",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run wpd | grunt fw",
    "wpd": "webpack --watch --config webpack.config.dev.js",
    "wp": "grunt uglify && webpack --config webpack.config.prod.js",
    "build": "npm run wp && grunt build",
    "css": "grunt s",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "js-toggler": "^1.1.0",
    "nu6tils": "git+https://github.com/romuleald/nu6tils.git#1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "cssnano": "^3.10.0",
    "grunt": "^1.0.1",
    "grunt-combine-media-queries": "^1.0.20",
    "grunt-contrib-uglify": "^2.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^2.0.0",
    "webpack": "^2.2.1"
  }
}
